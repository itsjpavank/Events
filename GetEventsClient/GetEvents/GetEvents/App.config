<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

  <appSettings>
    <!--<add key="EndPointUrl" value="http://pusheventsservice.azurewebsites.net/"/>-->
    <add key="AADInstance" value="https://login.windows.net/{0}"/>
    <add key="Tenant" value="microsoft.onmicrosoft.com"/>
    <!-- Client ID of the registered client application on AD -->
    <add key="ClientId" value="configure your clientid"/>
    <!-- APP ID URI of the Web API registered with AD -->
    <add key="ApiResourceId" value="https://geteventservicetest.onmicrosoft.com/geteventservicetest"/>
    <!-- Actual URL where Web API is hosted -->
    <add key="ApiBaseAddress" value="https://geteventsservicetest.one.microsoft.com/"/>
    <!-- Key for client authentication -->
    <add key="AppKey" value="configure your secret"/>
  
  </appSettings>
</configuration>